#!/bin/bash
##
## DESCRIPTION:
##      Script to print required mk targets to STDOUT
## USAGE:
##      bin/create-targets

# Generate targets for BAM files to estimate structural variation using Breakdancer.
find -L data/ \
	-type f \
	-name "*.bam" \
| sed \
	-e 's#data\/#results/#g' \
	-e 's#.bam#.ctx#g'
