#!/bin/bash
##
## DESCRIPTION:
##      Script to print required mk targets to STDOUT
## USAGE:
##      bin/create-targets

# Generate targets for VCF files to be converted to a BED format.
find -L data/ \
        -type f \
        -name "*.ctx" \
| sed \
        -e "s#data/#results/#" \
        -e "s#.ctx#_breakdancer.bed#" \

